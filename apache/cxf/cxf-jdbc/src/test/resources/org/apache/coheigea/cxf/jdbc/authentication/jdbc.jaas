
jetty {
     org.eclipse.jetty.jaas.spi.JDBCLoginModule required
     debug="true"
     dbUrl="jdbc:derby:memory:derbyDB"
     dbUserName="admin"
     dbPassword="security"
     dbDriver="org.apache.derby.jdbc.ClientDriver"
     userTable="app.users"
     userField="name"
     credentialField="password"
     userRoleTable="app.roles"
     userRoleUserField="name"
     userRoleRoleField="role";
};
